        <View android:background="@color/mj_color_separator" android:layout_width="match_parent" android:layout_height="0.1dp"/>
        <RelativeLayout android:id="@+id/layout_rn_plugin_demo" android:background="@drawable/selector_setting_main_page_item" android:layout_width="match_parent" android:layout_height="60dp">
