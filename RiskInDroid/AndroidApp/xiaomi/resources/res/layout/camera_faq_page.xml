    </LinearLayout>
    <com.xiaomi.smarthome.framework.webview.SmartHomeWebView android:id="@+id/webview" android:layout_width="match_parent" android:layout_height="match_parent" android:layout_above="@+id/bottom_bar" android:layout_below="@+id/list_space"/>
</RelativeLayout>
