<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" android:background="@color/window_background" android:layout_width="match_parent" android:layout_height="match_parent">
    <LinearLayout android:gravity="center_vertical" android:orientation="vertical" android:id="@+id/insurance_btnll" android:background="@color/white" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_alignParentBottom="true">
        <com.mi.global.shop.widget.CommonButton android:id="@+id/insurance_confirm" android:layout_width="match_parent" android:layout_marginLeft="15dp" android:layout_marginTop="10dp" android:layout_marginRight="15dp" android:text="@string/cart_insurance_confirm" style="@style/ProductDetailGotoCartButton"/>
        <com.mi.global.shop.widget.CommonButton android:id="@+id/insurance_cancel" android:layout_width="match_parent" android:layout_marginLeft="15dp" android:layout_marginTop="10dp" android:layout_marginRight="15dp" android:layout_marginBottom="20dp" android:text="@string/cart_insurance_cancel" style="@style/RegisterButton"/>
    </LinearLayout>
    <com.mi.global.shop.widget.BaseWebView android:scrollbarSize="0dp" android:id="@+id/cart_insurance_webview" android:scrollbars="none" android:layout_width="match_parent" android:layout_height="match_parent" android:layout_above="@+id/insurance_btnll"/>
</RelativeLayout>
