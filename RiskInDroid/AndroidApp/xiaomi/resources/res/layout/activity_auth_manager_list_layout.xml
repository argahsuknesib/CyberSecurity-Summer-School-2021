<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android" android:orientation="vertical" android:layout_width="match_parent" android:layout_height="match_parent">
    <include layout="@layout/mj_module_a_3_title_bar_return"/>
    <ListView android:id="@+id/auth_manager_list_lv" android:background="@color/mj_color_white" android:scrollbars="none" android:layout_width="match_parent" android:layout_height="match_parent" android:divider="@color/mj_color_black_00_transparent" android:layout_below="@+id/title_bar"/>
    <LinearLayout android:orientation="vertical" android:id="@+id/empty_ll" android:focusable="true" android:visibility="gone" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_centerInParent="true">
        <ImageView android:layout_gravity="center_horizontal" android:layout_width="wrap_content" android:layout_height="wrap_content" android:src="@drawable/std_main_icon_empty" android:importantForAccessibility="no"/>
        <TextView android:textSize="14dp" android:textColor="@color/mj_color_black_80_transparent" android:layout_gravity="center_horizontal" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="20dp" android:text="@string/no_auth_app"/>
    </LinearLayout>
</RelativeLayout>
