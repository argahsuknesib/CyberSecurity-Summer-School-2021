<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android" android:layout_width="match_parent" android:layout_height="@dimen/passport_title_bar_height" android:paddingStart="@dimen/passport_title_bar_padding_horizontal" android:paddingEnd="@dimen/passport_title_bar_padding_horizontal">
    <ImageView android:id="@+id/header_back" android:layout_width="@dimen/passport_title_bar_icon_size" android:layout_height="@dimen/passport_title_bar_icon_size" android:src="@drawable/passport_ic_back" android:layout_centerVertical="true" android:layout_alignParentStart="true"/>
    <ImageView android:id="@+id/header_help" android:layout_width="@dimen/passport_title_bar_icon_size" android:layout_height="@dimen/passport_title_bar_icon_size" android:src="@drawable/passport_ic_help" android:layout_centerVertical="true" android:layout_alignParentEnd="true"/>
    <TextView android:id="@+id/header_country_name" android:text="中国大陆" android:layout_centerVertical="true" android:layout_toStartOf="@+id/header_help" style="@style/Passport.HighlightText"/>
</RelativeLayout>
