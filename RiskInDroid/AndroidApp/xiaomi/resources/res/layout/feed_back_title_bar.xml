        <ImageView android:layout_gravity="top|right|center_vertical|center_horizontal|center" android:id="@+id/image_new_tag" android:visibility="gone" android:layout_width="wrap_content" android:layout_height="wrap_content" android:src="@drawable/common_point_now" android:importantForAccessibility="no"/>
