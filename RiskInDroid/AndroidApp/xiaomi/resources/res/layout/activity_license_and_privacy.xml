<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android" android:orientation="vertical" android:id="@+id/license_and_private" android:background="@color/mj_color_background" android:layout_width="match_parent" android:layout_height="match_parent">
    <include layout="@layout/mj_module_a_3_title_bar_return"/>
    <RelativeLayout android:id="@+id/license" android:background="@color/mj_color_white" android:layout_width="match_parent" android:layout_height="@dimen/std_list_item_height_single_line">
        <View android:background="@color/mj_color_separator" android:layout_width="match_parent" android:layout_height="1px" android:layout_marginLeft="@dimen/size_15dp" android:layout_alignParentBottom="true"/>
        <TextView android:textSize="@dimen/std_list_font_size_title" android:textColor="@color/mj_color_black_100_transparent" android:gravity="center_vertical" android:layout_width="wrap_content" android:layout_height="match_parent" android:layout_marginLeft="@dimen/size_15dp" android:text="@string/device_more_activity_license" android:layout_centerVertical="true" android:layout_alignParentStart="true"/>
        <ImageView android:layout_width="wrap_content" android:layout_height="wrap_content" android:src="@drawable/um_arrow" android:layout_centerVertical="true" android:importantForAccessibility="no" android:layout_marginEnd="20dp" android:layout_alignParentEnd="true"/>
    </RelativeLayout>
    <RelativeLayout android:id="@+id/privacy" android:background="@color/mj_color_white" android:layout_width="match_parent" android:layout_height="@dimen/std_list_item_height_single_line">
        <View android:background="@color/mj_color_separator" android:layout_width="match_parent" android:layout_height="1px" android:layout_marginLeft="@dimen/size_15dp" android:layout_alignParentBottom="true"/>
        <TextView android:textSize="@dimen/std_list_font_size_title" android:textColor="@color/mj_color_black_100_transparent" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginLeft="@dimen/dimen_15" android:text="@string/device_more_activity_privacy" android:layout_centerVertical="true"/>
        <ImageView android:layout_width="wrap_content" android:layout_height="wrap_content" android:src="@drawable/um_arrow" android:layout_centerVertical="true" android:importantForAccessibility="no" android:layout_marginEnd="20dp" android:layout_alignParentEnd="true"/>
    </RelativeLayout>
    <RelativeLayout android:id="@+id/usr_exp_plan_container" android:background="@drawable/mj_rs_common_white_list_padding_no_left_margin" android:visibility="gone" android:layout_width="match_parent" android:layout_height="@dimen/std_list_item_height_single_line">
        <TextView android:textSize="@dimen/std_list_font_size_title" android:textColor="@color/mj_color_black_100_transparent" android:gravity="left|center_vertical|center_horizontal|center" android:id="@+id/usr_exp_plan_container_tv" android:layout_width="match_parent" android:layout_height="wrap_content" android:text="@string/usr_exp_plan" android:layout_centerVertical="true" android:layout_toStartOf="@+id/right_arrow"/>
        <ImageView android:id="@+id/right_arrow" android:layout_width="wrap_content" android:layout_height="wrap_content" android:src="@drawable/um_arrow" android:layout_centerVertical="true" android:importantForAccessibility="no" android:layout_marginEnd="20dp" android:layout_alignParentEnd="true"/>
    </RelativeLayout>
    <View android:background="@color/mj_color_separator" android:layout_width="match_parent" android:layout_height="1px"/>
    <include android:id="@+id/list_space2" android:layout_width="match_parent" android:layout_height="@dimen/std_list_space_margin_top" layout="@layout/common_list_space_empty"/>
    <RelativeLayout android:id="@+id/cancel_license" android:background="@drawable/common_white_list_bg" android:layout_width="match_parent" android:layout_height="@dimen/setting_item_single_line_without_icon_height">
        <TextView android:textSize="@dimen/std_list_font_size_title" android:textColor="@color/mj_color_red_normal" android:ellipsize="end" android:gravity="center" android:id="@+id/cancel_license_btn" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="@string/device_more_activity_cancel_license_privacy" android:maxLines="2" android:layout_centerInParent="true" android:layout_marginEnd="@dimen/std_list_content_margin_right"/>
    </RelativeLayout>
    <View android:id="@+id/bottom_divider" android:background="@color/mj_color_separator" android:layout_width="match_parent" android:layout_height="1px"/>
</LinearLayout>
