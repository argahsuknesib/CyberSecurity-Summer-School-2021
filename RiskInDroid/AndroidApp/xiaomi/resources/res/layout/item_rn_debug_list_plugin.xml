        <LinearLayout android:gravity="center_vertical" android:orientation="vertical" android:id="@+id/layout_item_rn_debug_left" android:layout_width="0dp" android:layout_height="match_parent" android:layout_marginLeft="20dp" android:layout_weight="1">
            <TextView android:textSize="14sp" android:textColor="#666666" android:id="@+id/tv_debug_package_name" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="aaa"/>
            <TextView android:textSize="14sp" android:textColor="#666666" android:id="@+id/tv_debug_device_model" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="aaa"/>
        </LinearLayout>
