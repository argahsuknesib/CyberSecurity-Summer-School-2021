<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="true" android:state_pressed="true">
        <shape>
            <solid android:color="@color/mj_color_black_05_transparent"/>
            <corners android:radius="10dp"/>
            <stroke android:width="1dp" android:color="@color/mj_color_gray_lighter"/>
        </shape>
    </item>
    <item>
        <shape>
            <solid android:color="@color/mj_color_white"/>
            <corners android:radius="10dp"/>
            <stroke android:width="1dp" android:color="@color/mj_color_gray_lighter"/>
        </shape>
    </item>
</selector>
