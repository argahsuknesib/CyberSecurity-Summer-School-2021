<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape>
            <corners android:radius="5dp"/>
            <stroke android:width="0.3dp" android:color="@color/mj_color_green_normal"/>
            <size android:height="42dp" android:width="280dp"/>
        </shape>
    </item>
    <item>
        <shape>
            <corners android:radius="5dp"/>
            <stroke android:width="0.3dp" android:color="@color/mj_color_black_30_transparent"/>
            <size android:height="42dp" android:width="280dp"/>
        </shape>
    </item>
</selector>
