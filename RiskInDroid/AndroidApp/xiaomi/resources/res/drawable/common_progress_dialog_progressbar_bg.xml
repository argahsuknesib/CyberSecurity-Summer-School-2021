<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="8dp"/>
            <gradient android:startColor="#ffdfdfdf" android:endColor="#ffdfdfdf" android:angle="270"/>
        </shape>
    </item>
    <item android:id="@android:id/secondaryProgress">
        <shape>
            <corners android:radius="8dp"/>
            <gradient android:startColor="#ffdfdfdf" android:endColor="#ffdfdfdf" android:angle="270"/>
        </shape>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="8dp"/>
                <gradient android:startColor="#ff01db91" android:endColor="#ff01db91" android:angle="270" android:centerColor="#ff01db91"/>
            </shape>
        </clip>
    </item>
</layer-list>
